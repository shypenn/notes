{
	"env":{
		"browser":true
		,"node":true
		,"es6":true
	}
	,"parser":"babel-eslint"
	,"parserOptions":{
		"ecmaFeatures":{
			"impliedStrict":true
			,"jsx":true
		}
		,"sourceType":"module"
	}
	,"plugins":[
		"react",
	]
	,"rules":{
		"no-console":'off'
	}
	,"extends":["eslint:recommended", "plugin:react/recommended"]
}
